{
  "framework": null,
  "buildCommand": "echo 'Static deployment - no build required'",
  "outputDirectory": "public",
  "installCommand": "echo 'No install required'",
  "env": {
    "VITE_GOOGLE_MAPS_API_KEY": "@google_api_key"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}